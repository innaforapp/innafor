<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.hellocordova" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Innafor</name>
    <description>
        En applikasjon utviklet av studenter ved Universitetet i Agder for fotballklubben Jerv og Bl√• Kors Kristiansand.
    </description>
    <author email="post@innaforapp.no" href="http://innaforapp.no">
        Innafor
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="AutoHideSplashScreen" value="false" />
    <preference name="ShowSplashScreenSpinner" value="false" />
    <preference name="StatusBarOverlaysWebView" value="true" />
    <preference name="StatusBarStyle" value="darktranslucent" />
    <preference name="SplashScreen" value="/res/screen/browser/splashscreen.png" />
</widget>
